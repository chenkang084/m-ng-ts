{
  "name": "m-ng-ts",
  "version": "0.0.1",
  "description": "Angular 1.5 & Typescript",
  "main": "app.js",
  "scripts": {
    "start": "webpack-dashboard -- webpack-dev-server --content-base app/ --progress --colors --inline --hot --compress --history-api-fallback --port 8888",
    "setup": "npm install && typings install && gulp init",
    "clean-docs": "rimraf docs/**/*",
    "clean-build": "rimraf build/**/*",
    "clean": "npm run clean-docs && npm run clean-build",
    "build-prod": "npm run clean && webpack --prod --progress --colors && gulp build",
    "build-dev": "npm run clean && webpack --dev --progress --colors && gulp build",
    "test": "karma start"
  },
  "keywords": [
    "angular",
    "typescript",
    "bootstrap",
    "gulp",
    "webpack",
